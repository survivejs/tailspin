{
  "name": "tailwind-webpack-starter",
  "version": "0.0.0",
  "private": true,
  "description": "Opinionated starter for Tailwind/webpack projects",
  "main": "index.js",
  "scripts": {
    "build": "rimraf public && webpack --env production",
    "start": "webpack-dev-server --env development",
    "sitemap": "TODO",
    "check-links": "TODO",
    "check-types": "tsc --noEmit",
    "stats": "npm run build -- --json > stats.json",
    "analyze-stats": "webpack-bundle-analyzer stats.json"
  },
  "keywords": [
    "tailwind",
    "tailwindcss",
    "webpack"
  ],
  "author": "Juho Vepsäläinen",
  "license": "MIT",
  "devDependencies": {
    "@types/autoprefixer": "^9.6.1",
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/glob": "^7.1.1",
    "@types/mini-css-extract-plugin": "^0.9.0",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.1",
    "@types/rimraf": "^2.0.3",
    "@types/terser-webpack-plugin": "^2.2.0",
    "@types/webpack": "^4.41.5",
    "@types/webpack-bundle-analyzer": "^2.13.3",
    "@types/webpack-dev-server": "^3.10.0",
    "@types/webpack-merge": "^4.1.5",
    "@types/webpack-watched-glob-entries-plugin": "^2.1.0",
    "autoprefixer": "^9.7.4",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "decache": "^4.5.1",
    "glob": "^7.1.6",
    "husky": "^4.2.1",
    "hyperlink": "^4.4.3",
    "lint-staged": "^10.0.7",
    "mini-css-extract-plugin": "^0.9.0",
    "mini-html-webpack-plugin": "^3.0.7",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "purgecss-webpack-plugin": "^2.0.6",
    "rimraf": "^3.0.1",
    "sitemap-static": "^0.4.4",
    "tailwindcss": "^1.2.0",
    "tap-spot": "^1.1.1",
    "terser-webpack-plugin": "^2.3.4",
    "ts-loader": "^7.0.4",
    "ts-node": "^8.10.1",
    "typed-html": "^2.0.0",
    "typescript": "^3.8.3",
    "webpack": "^4.41.5",
    "webpack-add-dependency-plugin": "^0.1.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2",
    "webpack-watched-glob-entries-plugin": "^2.1.5"
  },
  "browserslist": [
    "defaults"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md}": "prettier --write"
  },
  "dependencies": {
    "sidewind": "^1.2.1"
  }
}
