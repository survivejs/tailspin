{
  "name": "tailwind-webpack-starter",
  "version": "0.0.0",
  "private": true,
  "description": "Opinionated starter for Tailwind/webpack projects",
  "main": "index.js",
  "scripts": {
    "build": "rimraf public && NODE_ENV=production webpack --env production",
    "start": "webpack-dev-server --env development",
    "sitemap": "TODO",
    "check-links": "TODO",
    "check-types": "tsc --noEmit",
    "stats": "npm run build -- --json > stats.json",
    "analyze-stats": "webpack-bundle-analyzer stats.json"
  },
  "keywords": [
    "tailwind",
    "tailwindcss",
    "webpack"
  ],
  "author": "Juho Vepsäläinen",
  "license": "MIT",
  "devDependencies": {
    "@types/autoprefixer": "^9.7.2",
    "@types/copy-webpack-plugin": "^6.0.0",
    "@types/glob": "^7.1.3",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.1",
    "@types/rimraf": "^3.0.0",
    "@types/terser-webpack-plugin": "^3.0.0",
    "@types/webpack": "^4.41.21",
    "@types/webpack-bundle-analyzer": "^3.8.0",
    "@types/webpack-dev-server": "^3.11.0",
    "@types/webpack-merge": "^4.1.5",
    "@types/webpack-watched-glob-entries-plugin": "^2.1.0",
    "autoprefixer": "^9.8.5",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^4.0.0",
    "decache": "^4.6.0",
    "glob": "^7.1.6",
    "husky": "^4.2.5",
    "hyperlink": "^4.5.0",
    "lint-staged": "^10.2.11",
    "mini-css-extract-plugin": "^0.9.0",
    "mini-html-webpack-plugin": "^3.0.7",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "sitemap-static": "^0.4.4",
    "tailwindcss": "^1.5.2",
    "tap-spot": "^1.1.1",
    "terser-webpack-plugin": "^3.0.7",
    "ts-loader": "^8.0.1",
    "ts-node": "^8.10.2",
    "typed-html": "^2.0.0",
    "typescript": "^3.9.7",
    "webpack": "^4.44.0",
    "webpack-add-dependency-plugin": "^0.1.2",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.0.9",
    "webpack-watched-glob-entries-plugin": "^2.1.6"
  },
  "browserslist": [
    "defaults"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md}": "prettier --write"
  },
  "dependencies": {
    "sidewind": "^1.2.1"
  }
}
