{
  "name": "tailwind-webpack-starter",
  "version": "0.0.0",
  "private": true,
  "description": "Opinionated starter for Tailwind projects",
  "main": "src/index.ts",
  "scripts": {
    "format": "deno fmt --unstable",
    "prebuild": "npm run generate-meta",
    "build": "npm run clean && TODO",
    "prestart": "npm run generate-meta",
    "start": "deno run --config ./tsconfig.json --unstable --allow-read --allow-write --allow-net src/index.ts --serve --watch",
    "clean": "rm -rf public",
    "generate-sitemap": "TODO",
    "check-links": "TODO",
    "generate-meta": "deno run --config ./tsconfig.json --unstable --allow-env --allow-plugin --allow-net --allow-read --allow-write ./utils/generate-meta.ts",
    "test": "deno test --config ./tsconfig.json --unstable --allow-env --allow-read --allow-write --allow-net --allow-plugin"
  },
  "keywords": [
    "tailwind",
    "tailwindcss"
  ],
  "author": "Juho Vepsäläinen",
  "license": "MIT",
  "devDependencies": {},
  "dependencies": {}
}
